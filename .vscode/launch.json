{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "extensionHost",
			"request": "launch",
			"name": "AVA Adapter",
			"runtimeExecutable": "${execPath}",
			"args": ["--extensionDevelopmentPath=${workspaceFolder}"],
			"sourceMaps": true,
			"outFiles": ["${workspaceFolder}/dist"]
		},
		{
			"type": "node",
			"request": "attach",
			"protocol": "inspector",
			"sourceMaps": true,
			"name": "Worker",
			"stopOnEntry": true,
			"skipFiles": [],
			"showAsyncStacks": true,
			"port": 9229
		}
	]
}
