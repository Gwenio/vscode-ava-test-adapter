{
	"compilerOptions": {
		"module": "esnext",
		"target": "es2018",
		"lib": [
			"es2016",
			"es2018.promise"
		],
		"moduleResolution": "node",
		"noImplicitAny": false,
		"sourceMap": true,
		"noEmitOnError": true,
		"strict": true,
		"alwaysStrict": true,
		"removeComments": true,
		"skipLibCheck": true
	}
}
