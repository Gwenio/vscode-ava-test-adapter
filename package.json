{
	"name": "vscode-ava-test-adapter",
	"displayName": "AVA Test Explorer",
	"description": "Run your AVA tests in the Sidebar of Visual Studio Code",
	"icon": "img/icon.png",
	"author": "James Adam Armstrong",
	"publisher": "gwenio",
	"version": "0.3.3",
	"license": "ISC",
	"homepage": "https://github.com/Gwenio/vscode-ava-test-adapter.git",
	"repository": {
		"type": "git",
		"url": "https://github.com/Gwenio/vscode-ava-test-adapter.git"
	},
	"bugs": {
		"url": "https://github.com/Gwenio/vscode-ava-test-adapter/issues"
	},
	"categories": ["Other"],
	"keywords": ["ava", "test", "testing"],
	"main": "dist/main.js",
	"scripts": {
		"clean": "rimraf tmp dist *.vsix",
		"compile": "tsc -p src/tsconfig.json",
		"coverage": "nyc ava --config ava.coverage.js",
		"rollup": "rollup -c",
		"build": "pnpm run compile && pnpm run rollup",
		"rebuild": "pnpm run clean && pnpm run build",
		"report": "nyc report -r text",
		"package": "vsce package --yarn",
		"publish": "vsce publish",
		"test": "ava"
	},
	"dependencies": {
		"arrify": "^2.0.1",
		"ava": "^2.2.0",
		"get-port": "^5.0.0",
		"matcher": "^2.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.5.5",
		"@babel/plugin-syntax-bigint": "^7.4.4",
		"@commitlint/cli": "^8.2.0",
		"@commitlint/config-conventional": "^8.2.0",
		"@sindresorhus/is": "^1.0.0",
		"@types/hash-sum": "^1.0.0",
		"@types/node": "^12.7.8",
		"@types/random": "^2.1.0",
		"@types/sinon": "^7.0.13",
		"@types/through2": "^2.0.34",
		"@types/vscode": "1.23.0",
		"@typescript-eslint/eslint-plugin": "^2.0.0",
		"@typescript-eslint/parser": "^2.0.0",
		"@typescript-eslint/typescript-estree": "^2.0.0",
		"babel-plugin-minify-dead-code-elimination": "^0.5.0",
		"babel-plugin-minify-guarded-expressions": "^0.4.3",
		"babel-plugin-transform-inline-consecutive-adds": "^0.4.3",
		"babel-plugin-transform-inline-environment-variables": "^0.4.3",
		"babel-plugin-transform-property-literals": "^6.9.4",
		"babel-plugin-transform-regexp-constructors": "^0.4.3",
		"babel-plugin-transform-remove-undefined": "^0.5.0",
		"builtin-modules": "^3.1.0",
		"chalk": "^2.4.2",
		"delay": "^4.3.0",
		"emittery": "^0.5.0",
		"eslint": "6.4.0",
		"eslint-config-prettier": "^6.1.0",
		"eslint-plugin-ava": "^9.0.0",
		"eslint-plugin-node": "^10.0.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-unicorn": "^12.0.0",
		"esm": "^3.2.25",
		"get-stream": "^5.1.0",
		"globby": "^10.0.1",
		"gulp": "^4.0.2",
		"gulp-babel": "^8.0.0",
		"gulp-filter": "^6.0.0",
		"gulp-plumber": "^1.2.1",
		"gulp-sourcemaps": "^2.6.5",
		"gulp-terser": "^1.2.0",
		"hash-sum": "^2.0.0",
		"husky": "^3.0.4",
		"json5": "^2.1.0",
		"lint-staged": "^9.2.5",
		"markdownlint-cli": "^0.18.0",
		"nyc": "^14.1.1",
		"ow": "^0.13.2",
		"prettier": "1.18.2",
		"random": "^2.1.1",
		"rimraf": "^3.0.0",
		"rollup": "^1.17.0",
		"rollup-plugin-alias": "^2.0.0",
		"rollup-plugin-babel": "^4.3.3",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-json": "^4.0.0",
		"rollup-plugin-license": "^0.12.1",
		"rollup-plugin-node-resolve": "^5.2.0",
		"rollup-plugin-sourcemaps": "^0.4.2",
		"rollup-plugin-terser": "^5.1.1",
		"rxjs": "^6.5.3",
		"sinon": "^7.4.1",
		"through2": "^3.0.1",
		"tslib": "^1.9.3",
		"typescript": "3.6.3",
		"veza": "^1.1.0",
		"vinyl-buffer": "^1.0.1",
		"vsce": "^1.65.0",
		"vscode-test-adapter-api": "^1.7.0",
		"vscode-test-adapter-util": "^0.7.0"
	},
	"engines": {
		"node": ">=10.16.0 <11 || >=12.7.0",
		"vscode": ">=1.23.0"
	},
	"greenkeeper": {
		"ignore": [
			"@types/vscode",
			"rimraf",
			"prettier",
			"husky",
			"lint-staged",
			"markdownlint-cli"
		],
		"commitMessages": {
			"initialBadge": "docs(readme): add greenkeeper badge",
			"initialDependencies": "chore(package) update dependencies",
			"initialBranches": "ci: whitelist greenkeeper branches",
			"dependencyUpdate": "chore(package): update ${dependency} to version ${version}",
			"devDependencyUpdate": "chore(package): update ${dependency} to version ${version}",
			"dependencyPin": "build(package): pin ${dependency} to ${oldVersion}",
			"devDependencyPin": "build(package): pin ${dependency} to ${oldVersion}"
		},
		"prTitles": {
			"initialPR": "Enable Greenkeeper",
			"initialPrBadge": "Add Greenkeeper badge to enable Greenkeeper",
			"initialPrBadgeOnly": "Add Greenkeeper badge",
			"initialSubgroupPR": "Update dependencies for ${group}",
			"basicPR": "Update ${dependency} to the latest",
			"groupPR": "Update ${dependency} in group ${group} to the latest"
		}
	},
	"esm": {
		"cjs": true
	},
	"husky": {
		"hooks": {
			"pre-commit": "npx lint-staged",
			"commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
		}
	},
	"lint-staged": {
		"*.js": ["prettier --write", "git add", "eslint"],
		"**.ts": ["prettier --write", "git add", "eslint"],
		"*.md": ["markdownlint -c ./.markdownlint.json"],
		"**.json": ["prettier --write", "git add"]
	},
	"extensionDependencies": ["hbenl.vscode-test-explorer"],
	"activationEvents": ["*"],
	"contributes": {
		"configuration": {
			"type": "object",
			"title": "AVA Test Explorer",
			"properties": {
				"avaExplorer.cwd": {
					"description": "The working directory where AVA is run (relative to the workspace folder)",
					"type": "string",
					"scope": "resource"
				},
				"avaExplorer.configs": {
					"description": "The location of the AVA config file (relative to the working directory)",
					"type": "array",
					"default": [
						{
							"file": null,
							"serial": false,
							"debuggerSkipFiles": []
						}
					],
					"items": {
						"description": "An AVA configuration.",
						"properties": {
							"file": {
								"description": "The configuration file relative to 'avaExplorer.cwd'.",
								"type": ["string", "null"],
								"default": null
							},
							"serial": {
								"description": "If true then test will be run serially.",
								"type": "boolean",
								"default": false
							},
							"debuggerSkipFiles": {
								"description": "An array of glob patterns for files to skip when debugging",
								"type": "array",
								"items": {
									"type": "string"
								},
								"default": []
							}
						}
					},
					"scope": "resource"
				},
				"avaExplorer.env": {
					"description": "Environment variables for the background worker.",
					"type": "object",
					"additionalProperties": {
						"type": "string"
					},
					"default": {},
					"scope": "resource"
				},
				"serialRuns": {
					"description": "If true then only one test or debug session will occur at a time.",
					"type": "boolean",
					"default": false
				},
				"avaExplorer.debuggerPort": {
					"description": "The preferred port to use for debugging sessions",
					"type": "number",
					"default": 9229,
					"scope": "resource"
				},
				"avaExplorer.nodePath": {
					"description": "The path to the node executable to use. By default it will attempt to find it on your PATH, if it can't find it or if this option is set to `null`, it will use the one shipped with VS Code",
					"type": ["string", "null"],
					"default": "default",
					"scope": "resource"
				},
				"avaExplorer.nodeArgv": {
					"description": "The arguments to the node executable to use. To enable typescript support, install the ts-node npm package and set this property to [\"-r\", \"ts-node/register\"]",
					"type": "array",
					"items": {
						"type": "string"
					},
					"scope": "resource"
				},
				"avaExplorer.debuggerSkipFiles": {
					"description": "An array of glob patterns for files to skip when debugging",
					"type": "array",
					"items": {
						"type": "string"
					},
					"scope": "resource"
				},
				"avaExplorer.workerTimeout": {
					"description": "How long to wait for the worker process to connect in milliseconds.",
					"type": "number",
					"minimum": 1000,
					"default": 10000,
					"maximum": 300000,
					"scope": "resource"
				},
				"avaExplorer.logpanel": {
					"description": "write diagnostic logs to an output panel",
					"type": "boolean",
					"scope": "resource"
				},
				"avaExplorer.logfile": {
					"description": "write diagnostic logs to the given file",
					"type": "string",
					"scope": "resource"
				}
			}
		}
	}
}
